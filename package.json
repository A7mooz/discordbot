{
	"name": "discordbot",
	"version": "1.2.0",
	"main": "./dist/index.js",
	"author": {
		"name": "EvolutionX-10",
		"url": "https://github.com/EvolutionX-10"
	},
	"license": "MIT",
	"packageManager": "yarn@3.2.2",
	"type": "module",
	"imports": {
		"#lib/*": "./dist/lib/*/index.js",
		"#core": "./dist/core/index.js",
		"#root/*": "./dist/*.js"
	},
	"scripts": {
		"dev": "yarn build && node .",
		"start": "node .",
		"build": "tsup",
		"typecheck": "tsc -b src",
		"watch": "tsup --watch --onSuccess=\"node .\""
	},
	"dependencies": {
		"colorette": "2.0.19",
		"discord-api-types": "0.37.10",
		"discord.js": "14.4.0",
		"dotenv": "16.0.2"
	},
	"devDependencies": {
		"@types/node": "18.7.8",
		"prettier": "2.7.1",
		"tsup": "6.2.3",
		"typescript": "4.8.3"
	}
}
