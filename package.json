{
	"name": "discordbot",
	"version": "1.5.0",
	"main": "./dist/index.js",
	"author": {
		"name": "EvolutionX-10",
		"url": "https://github.com/EvolutionX-10"
	},
	"license": "MIT",
	"packageManager": "yarn@3.5.0",
	"type": "module",
	"imports": {
		"#lib/*": "./dist/lib/*/index.js",
		"#core": "./dist/core/index.js",
		"#root/*": "./dist/*.js"
	},
	"scripts": {
		"dev": "yarn build && node --no-warnings .",
		"start": "node .",
		"build": "tsup",
		"typecheck": "tsc -b src",
		"watch": "tsup --watch --onSuccess=\"node .\""
	},
	"dependencies": {
		"colorette": "2.0.19",
		"discord-api-types": "0.37.37",
		"discord.js": "14.9.0",
		"dotenv": "16.0.3"
	},
	"devDependencies": {
		"@types/node": "18.15.11",
		"prettier": "2.8.7",
		"tsup": "6.7.0",
		"typescript": "5.0.3"
	}
}
